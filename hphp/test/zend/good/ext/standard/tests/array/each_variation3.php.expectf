*** Testing each() : usage variations ***

-- Iteration 1: int data --
array(4) {
  [1]=>
  string(4) "zero"
  ["value"]=>
  string(4) "zero"
  [0]=>
  int(0)
  ["key"]=>
  int(0)
}

-- Iteration 2: empty double quotes data --
array(4) {
  [1]=>
  string(6) "emptyd"
  ["value"]=>
  string(6) "emptyd"
  [0]=>
  string(0) ""
  ["key"]=>
  string(0) ""
}

-- Iteration 3: empty single quotes data --
array(4) {
  [1]=>
  string(6) "emptys"
  ["value"]=>
  string(6) "emptys"
  [0]=>
  string(0) ""
  ["key"]=>
  string(0) ""
}

-- Iteration 4: string data --
array(4) {
  [1]=>
  string(7) "stringd"
  ["value"]=>
  string(7) "stringd"
  [0]=>
  string(7) "stringd"
  ["key"]=>
  string(7) "stringd"
}
Done
